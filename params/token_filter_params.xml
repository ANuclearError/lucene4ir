<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<tokenFilters>
    <tokenizer>standard</tokenizer>
    <tokenFilter>
        <name>lowercase</name>
    </tokenFilter>
    <tokenFilter>
        <name>porterstem</name>
    </tokenFilter>
    <tokenFilter>
        <name>stop</name>
            <param>
                <key>ignoreCase</key>
                <value>false</value>
            </param>
            <param>
                <key>format</key>
                <value>wordset</value>
            </param>
    </tokenFilter>
</tokenFilters>